alias ..="cd .."
alias ls="lsd"

alias v="f -e nvim"
alias vim="nvim"

alias mount-secrets="gocryptfs ~/Sync/.secrets.enc ~/.secrets"
alias unmount-secrets="umount ~/.secrets"

alias av="source venv/bin/activate"
alias da="direnv allow"
alias tf="terraform"

alias gconfig="gcloud config configurations activate"
